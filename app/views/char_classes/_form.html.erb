<%= form_for @char do |form| %>
  <%= label_tag(:name, "Name:") %>
  <%= form.text_field :name %>
  <%= label_tag(:description, "Description:") %>
  <%= form.text_area :description %>
  <%= label_tag(:resource, "Resource:") %>
  <%= form.text_field :resource %>
  <%= form.submit @form_action %>
<% end %>

